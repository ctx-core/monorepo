{
	"name": "@ctx-core/monorepo",
	"version": "18.2.11",
	"description": "ctx-core monorepo",
	"keywords": [
		"ctx-core",
		"monorepo",
		"pnpm"
	],
	"homepage": "https://github.com/ctx-core/monorepo#readme",
